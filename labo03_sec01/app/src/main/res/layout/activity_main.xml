<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/cantidad_de_dinero"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.169" />

    <TextView
        android:id="@+id/numberText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:text="@string/textview2_placeholder"
        android:textColor="@color/black"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageView
        android:id="@+id/image_5_cents"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toStartOf="@+id/image_10_cents"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numberText"
        app:srcCompat="@drawable/five_cents" />

    <ImageView
        android:id="@+id/image_10_cents"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_5_cents"
        app:layout_constraintTop_toTopOf="@+id/image_5_cents"
        app:srcCompat="@drawable/ten_cents" />

    <ImageView
        android:id="@+id/image_25_cents"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toStartOf="@+id/image_one_dollar"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_5_cents"
        app:srcCompat="@drawable/quarter" />

    <ImageView
        android:id="@+id/image_one_dollar"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_25_cents"
        app:layout_constraintTop_toTopOf="@+id/image_25_cents"
        app:srcCompat="@drawable/one_dollar" />

    <TextView
        android:id="@+id/textView_5_cents"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/five_cents"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/image_25_cents"
        app:layout_constraintEnd_toEndOf="@+id/image_5_cents"
        app:layout_constraintStart_toStartOf="@+id/image_5_cents"
        app:layout_constraintTop_toBottomOf="@+id/image_5_cents" />

    <TextView
        android:id="@+id/textView_10_cents"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/ten_cents"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/image_one_dollar"
        app:layout_constraintEnd_toEndOf="@+id/image_10_cents"
        app:layout_constraintStart_toStartOf="@+id/image_10_cents"
        app:layout_constraintTop_toBottomOf="@+id/image_10_cents" />

    <TextView
        android:id="@+id/textView_25_cents"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/quarter"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/image_25_cents"
        app:layout_constraintStart_toStartOf="@+id/image_25_cents"
        app:layout_constraintTop_toBottomOf="@+id/image_25_cents" />

    <TextView
        android:id="@+id/textView_one_dollar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/one_dollar"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/image_one_dollar"
        app:layout_constraintStart_toStartOf="@+id/image_one_dollar"
        app:layout_constraintTop_toBottomOf="@+id/image_one_dollar" />

</androidx.constraintlayout.widget.ConstraintLayout>